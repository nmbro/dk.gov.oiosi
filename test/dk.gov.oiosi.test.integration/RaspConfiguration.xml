<?xml version="1.0" encoding="utf-8"?>
<RaspConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://oiosi.dk/rasp/xml/2007/04/01/">
  <ConfigurationSection xsi:type="LdapLookupFactoryConfig">
    <ImplementationNamespaceClass>dk.gov.oiosi.security.ldap.LdapCertificateLookup</ImplementationNamespaceClass>
    <ImplementationAssembly>dk.gov.oiosi.library</ImplementationAssembly>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="RevocationLookupFactoryConfig">
    <ImplementationNamespaceClass>dk.gov.oiosi.security.revocation.ocsp.OcspLookup</ImplementationNamespaceClass>
    <ImplementationAssembly>dk.gov.oiosi.library</ImplementationAssembly>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="UddiLookupClientFactoryConfig">
    <ImplementationNamespaceClass>dk.gov.oiosi.uddi.UddiLookupClient</ImplementationNamespaceClass>
    <ImplementationAssembly>dk.gov.oiosi.library</ImplementationAssembly>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="RegistryLookupClientFactoryConfig">
    <ImplementationNamespaceClass>dk.gov.oiosi.uddi.RegistryLookupClient</ImplementationNamespaceClass>
    <ImplementationAssembly>dk.gov.oiosi.library</ImplementationAssembly>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="LdapSettings">
    <Host>fenris.certifikat.dk</Host>
    <Port>389</Port>
    <ConnectionTimeoutMsec>5000</ConnectionTimeoutMsec>
    <SearchServerTimeoutMsec>5000</SearchServerTimeoutMsec>
    <SearchClientTimeoutMsec>5000</SearchClientTimeoutMsec>
    <MaxResults>1</MaxResults>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="OcspConfig">
    <DefaultTimeoutMsec>10000</DefaultTimeoutMsec>
    <ServerUrl>http://test.ocsp.certifikat.dk/ocsp/status</ServerUrl>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="SchematronStoreConfig">
    <MaxCompiledStylesheetsInMemory>2</MaxCompiledStylesheetsInMemory>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="UddiConfig">
    <ProfileConformanceClaim />
    <RegistrationConformanceClaim />
    <PublishEndpoint>https://publish.uddi.ehandel.gov.dk/UDDIProxy/UDDIProxy.svc</PublishEndpoint>
    <PublishInquiryEndpoint>http://publish.uddi.ehandel.gov.dk/registry/uddi/inquiry</PublishInquiryEndpoint>
    <SecurityEndpoint>http://publish.uddi.ehandel.gov.dk/registry/uddi/security</SecurityEndpoint>
    <FallbackTimeoutMinutes>15</FallbackTimeoutMinutes>
    <LookupRegistryFallbackConfig>
      <PrioritizedRegistryList>
        <Registry>
          <EndpointCollection>
            <Endpoint>http://discoverypublic.uddi.ehandel.gov.dk/registry/uddi/inquiry</Endpoint>
            <Endpoint>http://discovery.uddi.ehandel.gov.dk/registry/uddi/inquiry</Endpoint>
          </EndpointCollection>
        </Registry>
        <Registry>
          <EndpointCollection>
            <Endpoint>http://oiosi2vans.kmd.dk/UddiAdapterService/2008/10/27/UddiAdapterService.svc</Endpoint>
          </EndpointCollection>
        </Registry>
      </PrioritizedRegistryList>
    </LookupRegistryFallbackConfig>
    <TryOtherHostsOnFailure>true</TryOtherHostsOnFailure>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="RootCertificateConfig">
    <RootCertificateLocation>
      <StoreLocation>CurrentUser</StoreLocation>
      <StoreName>Root</StoreName>
      <SerialNumber>403617FC</SerialNumber>
    </RootCertificateLocation>
  </ConfigurationSection>
  <ConfigurationSection xsi:type="OcesX509CertificateConfig">
    <PersonalCertificateSubjectKey>
      <SubjectKeyString>PID</SubjectKeyString>
    </PersonalCertificateSubjectKey>
    <EmployeeCertificateSubjectKey>
      <SubjectKeyString>RID</SubjectKeyString>
    </EmployeeCertificateSubjectKey>
    <OrganizationCertificateSubjectKey>
      <SubjectKeyString>UID</SubjectKeyString>
    </OrganizationCertificateSubjectKey>
    <FunctionCertificateSubjetKey>
      <SubjectKeyString>FID</SubjectKeyString>
    </FunctionCertificateSubjetKey>
  </ConfigurationSection>
</RaspConfiguration>